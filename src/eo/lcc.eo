+package org.jpeek.calculus.eo
+alias org.jpeek.calculus.eo.np
+alias org.jpeek.calculus.eo.getConnectedPairs

[class] > lcc
  if. > @
    class.methods.length.geq 2
    div.
      toFloat.
        length.
          getConnectedPairs
            class.methods
      toFloat.
        np
          class.methods
    0.0

